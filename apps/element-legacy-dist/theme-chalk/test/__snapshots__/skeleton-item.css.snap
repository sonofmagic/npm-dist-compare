.el-skeleton__item {
  display: inline-block;
  width: 100%;
  height: 16px;
  background: #f2f2f2;
  border-radius: 4px;
}
.el-skeleton__circle {
  border-radius: 50%;
  width: 36px;
  height: 36px;
  line-height: 36px;
}
.el-skeleton__circle--lg {
  width: 40px;
  height: 40px;
  line-height: 40px;
}
.el-skeleton__circle--md {
  width: 28px;
  height: 28px;
  line-height: 28px;
}
.el-skeleton__button {
  width: 64px;
  height: 40px;
  border-radius: 4px;
}
.el-skeleton__p {
  width: 100%;
}
.el-skeleton__p.is-last {
  width: 61%;
}
.el-skeleton__p.is-first {
  width: 33%;
}
.el-skeleton__text {
  width: 100%;
  height: 13px;
}
.el-skeleton__caption {
  height: 12px;
}
.el-skeleton__h1 {
  height: 20px;
}
.el-skeleton__h3 {
  height: 18px;
}
.el-skeleton__h5 {
  height: 16px;
}
.el-skeleton__image {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  width: unset;
  border-radius: 0;
}
.el-skeleton__image svg {
  width: 22%;
  height: 22%;
  fill: #dcdde0;
}
